{
	"compilerOptions": {
        "incremental": true,
		"alwaysStrict": true,
		"declaration": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"forceConsistentCasingInFileNames": true,
		"lib": ["esnext"],
		"module": "commonjs",
		"moduleResolution": "node",
		"newLine": "LF",
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noUnusedParameters": true,
		"outDir": "./dist",
		"pretty": true,
		"removeComments": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"strictBindCallApply": true,
		"target": "esnext",
		"skipLibCheck": true,
        "types": ["@types/node", "@types/ws", "@types/natural", "discord.js", "klasa"]
	},
	"exclude": [
        "node_modules"
	],
	"include": [
		"./src/**/*",
	]
}